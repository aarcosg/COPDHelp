<?xml version="1.0" encoding="utf-8"?>
<com.github.clans.fab.FloatingActionMenu
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/fab_multi_actions_patient"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="bottom|end"
    android:layout_margin="@dimen/fab_margin"
    app:menu_colorNormal="@color/fab_normal"
    app:menu_colorPressed="@color/fab_pressed"
    app:menu_colorRipple="@color/fab_ripple">

    <com.github.clans.fab.FloatingActionButton
        android:id="@+id/action_medical_attention"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:fab_label="@string/medical_attention"
        app:fab_size="mini"
        app:fab_colorNormal="@color/fab_normal"
        app:fab_colorPressed="@color/fab_pressed"
        app:fab_colorRipple="@color/fab_ripple"
        app:ico_icon="gmd-local-hospital"
        app:ico_color="@color/icon_light"
        app:ico_size="@dimen/fab_mini_icon_size"/>

</com.github.clans.fab.FloatingActionMenu>